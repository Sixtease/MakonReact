{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint config src",
    "lint:fix": "yarn run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "just-deploy": "better-npm-run just-deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "just-deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run just-deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run just-deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "audio-buffer-utils": "^5.1.1",
    "audiobuffer-to-wav": "^1.0.0",
    "axios": "^0.17.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.1.0",
    "canvas-equalizer": "^0.2.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.7",
    "cssnano": "^3.7.4",
    "debug": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "fetch-jsonp": "^1.0.6",
    "file-loader": "^1.1.5",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.2",
    "json-loader": "^0.5.7",
    "node-sass": "4.7.2",
    "normalize.css": "^7.0.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^3.1.4",
    "react-redux": "^5.0.1",
    "react-router": "4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-form": "^7.1.1",
    "redux-thunk": "^2.0.0",
    "reselect": "^3.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "yargs": "^10.1.1",
    "yarn": "^1.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "nodemon": "^1.10.2",
    "phantomjs-prebuilt": "^2.1.12",
    "redbox-react": "^1.2.10",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.20.0"
  }
}
